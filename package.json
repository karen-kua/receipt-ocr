    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "install": "^0.12.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
